{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": false,
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  